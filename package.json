{
  "name": "sham-ui",
  "version": "1.2.2",
  "description": "Components managment library",
  "main": "lib/shamUI.min.js",
  "browser": "lib/shamUI.min.js",
  "scripts": {
    "test": "for f in test/*/index.html; do ./node_modules/mocha-phantomjs/bin/mocha-phantomjs $f; done",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors",
    "server": "webpack-dev-server",
    "build": "WEBPACK_ENV=build webpack",
    "prepublish": "npm run build",
    "doc": "esdoc"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "esdoc": "^0.5.2",
    "mocha": "^3.2.0",
    "mocha-phantomjs": "^4.1.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.22.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-es2015-modules-amd"
    ]
  }
}
