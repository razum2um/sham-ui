{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": [ "src" ],
    "exclude": [ "src/lib" ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "opts": {
    "recurse": true,
    "destination": "./docs/"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}